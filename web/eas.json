{
  "cli": { 
    "version": ">= 16.8.0", 
    "appVersionSource": "remote" 
  },
  "build": {
    "development": { 
      "developmentClient": true, 
      "distribution": "internal" 
    },
    "preview": { 
      "distribution": "internal" 
    },
    "production": { 
      "autoIncrement": true 
    },
    "e2e-test": {
      "withoutCredentials": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "submit": { 
    "production": {} 
  }
}